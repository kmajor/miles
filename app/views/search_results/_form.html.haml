= form_for @search_result do |f|
  - if @search_result.errors.any?
    #error_explanation
      %h2= "#{pluralize(@search_result.errors.count, "error")} prohibited this search_result from being saved:"
      %ul
        - @search_result.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :search
    = f.text_field :search
  .field
    = f.label :airline
    = f.text_field :airline
  .field
    = f.label :results
    = f.text_area :results
  .actions
    = f.submit 'Save'
